@page "/ReportesGerenteGenerar"

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">  
    <title>Document</title>
</head>

<nav class="navbar navbar-default">
    <div class="container-fluid">
      <div class="navbar-header">
        <a class="navbar-brand" href="#">Generar Reporte</a>
      </div>
    </div>
  </nav>

<body>
    <div class="container my-4">
        <form class="px-3">
          <div class="mb-3">
            <label for="inputFechaInicio">Fecha de inicio</label>
            <input type="date" class="form-control" id="inputFechaInicio">
          </div>
          <div class="mb-3">
            <label for="inputFechaFin">Fecha de fin</label>
            <input type="date" class="form-control" id="inputFechaFin">
          </div>
          <div class="mb-3">
            <label for="inputTipoPrestamo">Tipo de préstamo</label>
            <select class="form-control" id="inputTipoPrestamo">
              <option value="">Seleccionar tipo de préstamo</option>
              <option value="tipo1">Tipo 1</option>
              <option value="tipo2">Tipo 2</option>
              <option value="tipo3">Tipo 3</option>
            </select>
          </div>
          <div class="mb-3">
            <label for="inputCliente">Cliente</label>
            <input type="text" class="form-control" id="inputCliente">
          </div>
          <button type="submit" class="btn btn-primary">Generar reporte</button>
        </form>
      </div>
      
</body>

